	<xs:complexType name="VesselEventStatusSelectorAssembly_3">
		<xs:annotation>
			<xs:documentation>Allows for the selection of a vessel status or vessel event. | New status of a barge or service that was performed on a barge that caused a change in status.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Assist" type="VesselEventStatusAssembly_29" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A secondary boat (e.g. a fleet boat) has helped a primary boat (e.g. a line boat) with a task and a barge was involved.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BargeOnOrder" type="VesselEventStatusAssembly_26" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A request has been made to place a barge at a dock for loading or unloading.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Clean" type="VesselEventStatusAssembly_32" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has been cleaned by a vendor.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ConsignmentLocationChange" type="VesselEventStatusAssembly_26" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The current consignment location for a barge has changed.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CoverHandling" type="VesselEventStatusAssembly_33" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The covers of a barge have been physically manipulated in some manner.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Delay" type="VesselEventStatusAssembly_40" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Barge is delayed.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Fleeting" type="VesselEventStatusAssembly_37" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Barge is fleeting.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Load" type="VesselEventStatusAssembly_27" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had commodity placed within the cargo hold compartment.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MidstreamOperation" type="VesselEventStatusAssembly_25" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Midstream In (Drop) - A barge has been retrieved from the tow of a mainline boat. Midstream Out (Pickup) - A barge has been positioned to be included in the tow for a mainline boat.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Other" type="VesselEventStatusAssembly_34" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A miscellaneous event or status update.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Placement" type="VesselEventStatusAssembly_28" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has been positioned for a customer so it can be loaded or unloaded.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Position" type="VesselEventStatusAssembly_39" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Indicates the position of a barge at the given date and time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Pump" type="VesselEventStatusAssembly_35" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had water removed from the void tank areas or cargo hold area.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Released" type="VesselEventStatusAssembly_26" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has been approved to be removed from a dock area.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Repair" type="VesselEventStatusAssembly_36" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had some type of work performed to correct a physical problem.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Shift" type="VesselEventStatusAssembly_24" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge is scheduled to be, is in the process of, or has been moved from one location to another by a vendor.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TowAssist" type="VesselEventStatusAssembly_30" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A secondary boat has assisted a mainline boat with the making or breaking of a tow.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Unload" type="VesselEventStatusAssembly_31" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had commodity removed from the cargo hold compartment.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
