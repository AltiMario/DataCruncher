<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns="urn:x12:schemas:V00201202:BargeStatusDocument" xmlns:X12.7="urn:x12:schemas:V00201002:X12.7" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:x12:schemas:V00201202:BargeStatusDocument" elementFormDefault="qualified" attributeFormDefault="unqualified" version="00201202" X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="BargeStatusDocument">
		<xs:annotation>
			<xs:documentation>Template provides status information when the triggering event occurs. | The Barge Status Document provides a means for a Marine Trading Partner to transmit the status of a barge to another Marine Trading Partner. Status types include Clean, Load, Unload, Shift, Midstream, Pump, Repair, etc.  Each Status message may relate to a Barge Service Order Document. Each Status Document may also relate to a Barge Invoice Document Line Item when the Service Provider bills for service rendered.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DocumentHeader" type="StandardDocumentHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information for this document.  Uniquely identifies this document being transmitted.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicationHeader" type="MarineUpdatableApplicationHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information and uniquely identifies a document from an application's perspective.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EventInformation" type="MarineBargeStatusInformationModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Details about the new status of the barge.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Equipment" type="MarineBargeEquipmentInformationModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>The equipment whose status is being updated.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
	<xs:complexType name="MarineUpdatableApplicationHeaderModule">
		<xs:annotation>
			<xs:documentation>Uniquely identifies a transaction from an application's perspective. Provides a mechanism for indicating if a transaction is a new transaction, an update to a previously sent transaction, or if a previously sent transaction should be disregarded.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDetail" type="DocumentDetailBlock_37" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides control information and uniquely identifies the transaction that is represented by this document from an application's perspective. Provides a mechanism for indicating if a document is a new document, an update to a previously sent document, or if a previously sent document should be disregarded.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleLocation" type="LocationIdentifiersBlock_13" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location (e.g. fleet, terminal, lock, etc.) that is responsible for this document.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineBargeEquipmentInformationModule">
		<xs:annotation>
			<xs:documentation>Uniquely identifies barge.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Barge" type="MarineVesselBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies a barge.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineBargeStatusInformationModule">
		<xs:annotation>
			<xs:documentation>This module identifies barge status message details, including the type of status message that is being transmitted.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Status" type="VesselEventStatusSelectorAssembly_3" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The event that caused the new barge status.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimatedCostInformation" type="MarineBillingInformationAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The estimated cost of the event, if applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderDocumentReference" type="DocumentDetailBlock_33" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The order that directed the service provider to perform the service on the barge, if applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FleetBoat" type="MarineVesselBlock_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Fleet boat that performed the event, if applicable.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StandardDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Standard document header module</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who sends the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Receiver" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who receives the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of document</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
