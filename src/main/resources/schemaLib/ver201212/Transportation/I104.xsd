<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns="urn:x12:schemas:V00201202:BargeServiceOrderDocument" xmlns:X12.7="urn:x12:schemas:V00201002:X12.7" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:x12:schemas:V00201202:BargeServiceOrderDocument" elementFormDefault="qualified" attributeFormDefault="unqualified" version="00201202" X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="BargeServiceOrderDocument">
		<xs:annotation>
			<xs:documentation>Template provides equipment disposition request and response information. | The Barge Service Order Document provides a means for a Marine Trading Partner to transmit an order for services to be performed on one or more barges to a another Marine Trading Partner.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DocumentHeader" type="StandardDocumentHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information for this document.  Uniquely identifies this document being transmitted.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicationHeader" type="MarineApplicationHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information and uniquely identifies a document from an application's perspective.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Equipment" type="MarineOrderEquipmentInformationModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Equipment to which services should be performed.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OrderInformation" type="MarineOrderInformationModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Represents order details, such as the order number and line items.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="LineItem" type="MarineOrderLineItemModule" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of order line items.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
	<xs:complexType name="MarineOrderEquipmentInformationModule">
		<xs:annotation>
			<xs:documentation>Equipment to which services should be performed.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="SpecificBarge" type="VesselInformationAssembly_1" minOccurs="1" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Identifies a list of specific barges.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AnyBarge" type="VesselInformationAssembly_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The count of how many barges should be selected along with a list of characteristics the barges must meet.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineOrderInformationModule">
		<xs:annotation>
			<xs:documentation>This module identifies order message details, including the order number and the line item details.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrderIdentification" type="DocumentDetailBlock_38" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Order details, including date time, order number, and revision number.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineOrderLineItemModule">
		<xs:annotation>
			<xs:documentation>Contains the details about a marine order's line item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Details" type="LineItemBlock_2" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Order line item details, including the line item number and the estimated date time that the line item should be completed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="VesselEventStatusSelectorAssembly_2" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Order line item type.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StandardDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Standard document header module</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who sends the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Receiver" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who receives the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of document</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineApplicationHeaderModule">
		<xs:annotation>
			<xs:documentation>Provides control information and uniquely identifies a document from an application's perspective.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDetail" type="DocumentDetailBlock_36" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides control information and uniquely identifies the transaction that is represented by this document from an application's perspective.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleLocation" type="LocationIdentifiersBlock_13" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location (e.g. fleet, terminal, lock, etc.) that is responsible for this document.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Assemblies ========== -->
	<xs:complexType name="OrganizationContactAssembly_12">
