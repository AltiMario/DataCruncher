<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="urn:x12:schemas:V00201202:RequisitionDocument" xmlns:X12.7="urn:x12:schemas:V00201002:X12.7" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:x12:schemas:V00201202:RequisitionDocument" elementFormDefault="qualified" attributeFormDefault="unqualified" version="00201202" X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="RequisitionDocument">
		<xs:annotation>
			<xs:documentation>Conveys a description and quantity of the required material, delivery and billing information, and other logistics information necessary to evaluate and meet the material needs of the originator. | The main purpose of the document is to inquire about the status of, amend, or terminate the original requisition.This document may be used to convey a description and quantity of the required material, delivery and billing information, and other logistics information necessary to evaluate and meet the material needs of the originator. This document is not used to establish a contractual relationship between trading partners, and may precede the use of a purchase order.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="StandardDocumentHeader" type="StandardDocumentHeaderModule" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Used to convey the unique Header information for this Requisition Document.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DoDEnterpriseDocumentHeader" type="DoDDocumentHeaderModule" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Used to convey Enterprise unique header information for this Requisition Document.  Contains information unique to the US Department of Defense Document, such as Document Identification and date.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RequisitioningActivity" type="DoDTradingPartnerModule" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies the authoritative party who is requisitioning this item.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IntermediateReferralProcessingParty" type="DoDTradingPartnerModule" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The party receiving the request for processing, when the requisition is forwarded for further processing.  Also used to identify a party transmitting a requisition image transaction.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DeliveryInformation" type="DeliveryInformationModule" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>In the context of the US Department of Defense, identifies and describes delivery information of an item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="FinancialManagementInformation" type="DoDFinancialManagementModule" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Information relating to financial aspects.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ContractManagement" type="DoDProcurementManagementModule" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Contractual information relating to the item.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ItemInformation" type="DoDItemInformationModule" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Identifies the item being requisitioned.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="InventoryManagement" type="DoDMaterialManagementModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Inventory supply-based management information, such as who has responsibility to manage the material being requested.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="MaintenanceManagement" type="DoDMaintenanceModule" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>US Department of Defense maintenance information.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
	<xs:complexType name="DoDTradingPartnerModule">
		<xs:annotation>
			<xs:documentation>DoD identification of a Government organization, commercial trading partner, their address, POC and their geographical location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_7" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationLocation" type="EntityLocationAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Location of organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrimaryContactInformation" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The primary point-of-contact for an organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AlternateContactInformation" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The alternate point(s)-of-contact for an organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDItemInformationModule">
		<xs:annotation>
			<xs:documentation>In the context of the US Department of Defense, identifies and describes an item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ItemIdentification" type="ItemIdentificationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Uniquely identifies the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ItemProperties" type="PhysicalPropertiesBlock_8" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes the quantity, unit-of-issue, and other properties of the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CrossReferenceIdentification" type="ItemIdentificationBlock_4" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies cross reference information to another transaction or document.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimatedPrice" type="AmountBlock_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The estimated price of the item in dollars and cents.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContractPrice" type="AmountBlock_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The contract price of the item in dollars and cents.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BilledPrice" type="AmountBlock_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The billed price of the item in dollars and cents.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActualPrice" type="AmountBlock_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The actual price of the item in dollars and cents.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Information about the US Department of Defense Document itself, such as Document Identification and date.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_11" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details specific to ithe document itself, such as identification and document creation date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InformationCopy" type="TradingPartnerAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of an organization, commercial trading partner, its address, POC and its geographical location.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDMaintenanceModule">
		<xs:annotation>
			<xs:documentation>US Department of Defense maintenance information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="WarrantyDetail" type="DocumentDetailBlock_6" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The warranty information for material in maintenance.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaintenanceWorkOrderDetails" type="DocumentDetailBlock_30" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Maintenace Work Order Details. Within the US Department of Defense the work order is the work order for maintenance repairs issued by a maintenance activity.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDMaterialManagementModule">
		<xs:annotation>
			<xs:documentation>US Department of Defense information about the management aspects of material in supply.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Manager" type="OrganizationBlock_14" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies the party (or parties) assigned to manage this item.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MaterialManagementDetail" type="MaterialManagementBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The details of the management of material in supply.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CurrentCharacteristics" type="ItemCharacteristicsBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Describes the current characteristics of the material, used to identify how the material is managed and used.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="DoDWeaponSystemBreakout" type="SystemBreakoutAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>United States Department of Defense Weapon System Breakout.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDProcurementManagementModule">
		<xs:annotation>
			<xs:documentation>US Department of Defense procurement information, such as order and contract identification.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ContractInformation" type="DocumentDetailBlock_5" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies and describes the US Department of Defense order information for this item.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrderInformation" type="DocumentDetailBlock_19" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies and describes the US Department of Defense order information for this item.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ManufacturerDirectiveNumber" type="DocumentDetailBlock_5" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies and describes the US Department of Defense Manufacturer's Directive.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="ManagmentControlActivity" type="TradingPartnerAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party (or parties) assigned to manage this item</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DoDFinancialManagementModule">
		<xs:annotation>
			<xs:documentation>US Department of Defense information about the financial management of material or service, such as source of funding.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FinancialManagement" type="AccountInformationBlock_2" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Specifies funding by means of a code, which describes how funding will be provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillToParty" type="TradingPartnerAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The Bill-to party.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StandardDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Standard document header module</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who sends the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Receiver" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who receives the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of document</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeliveryInformationModule">
		<xs:annotation>
			<xs:documentation>In the context of the US Department of Defense, identifies and describes delivery information of an item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ShipToParty" type="TradingPartnerAssembly_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies the party to whom the item must be shipped.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DeliveryDateInformation" type="EventRangeBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Dates relating to a delivery event or period.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Assemblies ========== -->
	<xs:complexType name="OrganizationContactAssembly_12">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an organization. | Information used to contact the organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_15" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressBlock_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes what is commonly known as the mailing address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualContactAssembly_2">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an individual. | Information used to contact the individual.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ContactIdentification" type="IndividualBlock_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndividualLocation" type="EntityLocationAssembly_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Location of Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItemIdentificationAssembly_1">
		<xs:annotation>
			<xs:documentation>Identifies a specific item by both commodity and unique identifier | Within the context of the US Department of Defense, information that provides both commodity identification, such as part number, and unique identification, such as serial number, to an item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ItemIdentifier" type="ItemIdentificationBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identification of the part by either NSN (National Stock Number) or Part Number.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SerializedItemIdentifier" type="ItemIdentificationBlock_3" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies the item by a unique, serial or catalog number.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SystemBreakoutAssembly_1">
		<xs:annotation>
			<xs:documentation>Identifies the parts of a system (e.g., parts breakout, line item detail resource designation, ownership). | United States Department of Defense Weapon System Breakout.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="WeaponSystemSerializedItemIdentification" type="ItemIdentificationBlock_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifies the Weapon System by a unique, serial or catalog number (not the part on the weapon system).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="WeaponSystemDetails" type="ItemCharacteristicsBlock_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Identifes the Department of Defense Weapon System details (e.g., Weapon System Designator, Service or Agency Code and weapon system quantity.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TradingPartnerAssembly_1">
		<xs:annotation>
			<xs:documentation>Identifies contact information of party. | Identifies contact information of party.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="DoDOrganizationIdentifier" type="OrganizationBlock_7" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The US Department of Defense Organizational Identifier.  Specifies the type and identity of an organization.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="OrganizationLocation" type="EntityLocationAssembly_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Combination of information that defines a specified individual's or organization's electronic and physical location.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndividualContactAssembly" type="IndividualContactAssembly_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Assembly of individual contact information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndividualContactAssembly2" type="IndividualContactAssembly_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Another assembly of individual contact information.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrganizationAndIndividualInformationAssembly_1">
		<xs:annotation>
			<xs:documentation>Organization and Individual Information Assembly | Identifies the organization and provides an individual contact.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationContact" type="OrganizationContactAssembly_12" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualContact" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Individual involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EntityLocationAssembly_2">
		<xs:annotation>
			<xs:documentation>Combination of information that defines a specified individual's or organization's electronic and physical location. | Combination of information that defines a specified individual's or organization's electronic and physical location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" type="AddressBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes what is commonly known as the mailing address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ElectronicLocation" type="ElectronicLocationBlock_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Combination of information that defines a specified individual's or organization's electronic location.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TelephoneDetail" type="TelephoneBlock_1" minOccurs="0" maxOccurs="3">
				<xs:annotation>
					<xs:documentation>Comprehensive telephone block, designed to provide definition for the telephone number and a variety of characteristics covering use and features.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EntityLocationAssembly_4">
		<xs:annotation>
			<xs:documentation>Combination of information that defines a specified individual's or organization's electronic and physical location. | Combination of information that defines a specified individual's or organization's electronic and physical location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ElectronicLocation" type="ElectronicLocationBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Combination of information that defines a specified individual's or organization's electronic location.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Telephone" type="TelephoneBlock_1" minOccurs="1" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>Comprehensive telephone block, designed to provide definition for the telephone number, and also a variety of characteristics covering use and features.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Blocks ========== -->
	<xs:complexType name="AmountBlock_2">
		<xs:annotation>
			<xs:documentation>Describes the value and purpose of a monetary amount. Can also optionally include the value and reason for adjustments to an amount.  All concrete amount blocks must clearly state the purpose and effect of adjustments (indicate if an amount includes an ad | The value of the item in dollars and cents</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Amount" type="Amount_5" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit Price of item in dollars and cents</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PhysicalPropertiesBlock_8">
		<xs:annotation>
			<xs:documentation>Characteristics of an item(s) that are based on an objective scale of  measurement.  Non-measurable item(s) should be considered 'Qualities'. | Describes the properties of the item</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Quantity" type="Quantity_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Number of items being referenced.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Weight" type="WeightMeasure_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Weight of the items being referenced.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Volume" type="VolumeMeasure_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unit cubic volume of item.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocumentDetailBlock_11">
		<xs:annotation>
			<xs:documentation>Specifies a logical or physical document level of contextual information, unique identification number, creation date, etc. | US Department of Defense-specific details of the Document itself, such as Document identification and date of creation.</xs:documentation>
