					<xs:annotation>
						<xs:documentation>PCard (Purchase Card) Transaction detail information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PCardAuthorizationInformation" type="PCardAVSInformationModule" minOccurs="1" maxOccurs="1">
