<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="urn:x12:schemas:V00201202:PCardVisaPaymentDocument" xmlns:X12.7="urn:x12:schemas:V00201002:X12.7" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:x12:schemas:V00201202:PCardVisaPaymentDocument" elementFormDefault="qualified" attributeFormDefault="unqualified" version="00201202" X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="PCardVisaPaymentDocument">
		<xs:annotation>
			<xs:documentation>PCard (Purchase Card) transaction for payment. | Process to create VISA PCard (Purchase Card) transactions for payment. PCard refers to Business Accounts, either via a card or electronic account.  These can be held Corporately or per individual within Company.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
