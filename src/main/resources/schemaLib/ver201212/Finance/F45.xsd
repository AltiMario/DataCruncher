						<xs:documentation>PCard (Purchase Card) batch authorization information.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PCardLevel2Detail" type="PCardLevel2DetailVisaModule" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Level 2 Data elements for PCard (Purchase Card) transaction.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PCardLevel3LineItemDetail" type="PCardLevel3LineItemDetailForVisaPaymentsModule" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Line item detail within PCard (Purchase Card) transaction.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
