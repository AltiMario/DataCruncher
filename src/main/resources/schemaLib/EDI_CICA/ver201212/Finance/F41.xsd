version="00201202" 
X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="AchCcd">
		<xs:annotation>
			<xs:documentation>Template that includes all forms of payment and collection. | An ACH entry that identifies debits and credits initiated by an originator to effect a transfer of funds to or from the account of that organization or another organization.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DocumentHeader" type="StandardDocumentHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information for this document. | Uniquely identifies this document being transmitted.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AchCcdPaymentOrder" type="AchCcd.Module" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Use this slot to identify the payment clearing system to be used and the payment information required for that specific clearing system.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
	<xs:complexType name="AchCcd.Module">
		<xs:annotation>
			<xs:documentation>Use this module to identify the payment clearing system to be used and the payment information required for that specific clearing system.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchCcdMonetaryDetail" type="AchCcdTransactionDetail.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Detail needed to process the monetary section of an ACH CCD entry.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchCcdOriginatorDetail" type="AchCcdOriginator.Assembly" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Detail needed to process the Orginator section of an ACH entry.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchCcdFinancialReceiver" type="AchFinancialReceiver.Assembly" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Detail Needed to process the receiver section of an ACH entry.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StandardDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Standard document header module</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who sends the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Receiver" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who receives the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of document</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Assemblies ========== -->
	<xs:complexType name="OrganizationContactAssembly_12">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an organization. | Information used to contact the organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_15" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressBlock_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes what is commonly known as the mailing address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualContactAssembly_2">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an individual. | Information used to contact the individual.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ContactIdentification" type="IndividualBlock_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndividualLocation" type="EntityLocationAssembly_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Location of Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrganizationAndIndividualInformationAssembly_1">
		<xs:annotation>
			<xs:documentation>Organization and Individual Information Assembly | Identifies the organization and provides an individual contact.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationContact" type="OrganizationContactAssembly_12" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualContact" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Individual involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchCcdOriginator.Assembly">
		<xs:annotation>
			<xs:documentation>Information on the originator of a financial transaction. | Detail needed to process the Orginator section of an ACH entry.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchCcdOriginatorOrganization" type="AchOriginatorCompany.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name and identification of the Originator company.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchOriginatorAccountDetail" type="OriginatorAccountDetail.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the originating account.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchFinancialReceiver.Assembly">
		<xs:annotation>
			<xs:documentation>Information on the receiver of a financial transaction. | Detail Needed to process the receiver section of an ACH entry.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchReceiverAccountDetail" type="ReceiverAccountDetail.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the receiver's account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchReceiverPaymentReference" type="AchCcdPaymentReference.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This is an ACH (CCD, CTX, CIE) payment reference number that uniquely identifies this entry so it can be reassociated with it's remittance information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchReceiverOrganization" type="AchCcdReceiverOrganization.Block" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the receiving organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EntityLocationAssembly_4">
		<xs:annotation>
			<xs:documentation>Combination of information that defines a specified individual's or organization's electronic and physical location. | Combination of information that defines a specified individual's or organization's electronic and physical location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ElectronicLocation" type="ElectronicLocationBlock_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Combination of information that defines a specified individual's or organization's electronic location.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Telephone" type="TelephoneBlock_1" minOccurs="1" maxOccurs="5">
					<xs:annotation>
						<xs:documentation>Comprehensive telephone block, designed to provide definition for the telephone number, and also a variety of characteristics covering use and features.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Blocks ========== -->
	<xs:complexType name="AchCcdPaymentReference.Block">
		<xs:annotation>
			<xs:documentation>Combination of constructs that together identify a specific resource. | This is an ACH payment reference number that uniquely identifies this entry so it can be reassociated with its remittance information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchCcdReceiverPaymentReference" type="AchCcdReceiverPayment.Identifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This is an ACH CCD payment reference number that uniquely identifies this entry so it can be reassociated with it's remittance information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DocumentDetailBlock_31">
		<xs:annotation>
			<xs:documentation>Specifies a logical or physical document level of contextual information, unique identification number, creation date, etc. | Identification of document.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CreateDateAndTime" type="DateTime_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The date and time that this instance of the document is generated.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="InstanceIdentifier" type="ResourceIdentifier_85" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>An instance identifier is a business document or an arbitrary control number.  The combination of the instance identifier and the creation date and time must be unique in the eyes of the receiver.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressBlock_1">
		<xs:annotation>
			<xs:documentation>Grouping of information used to specify the location address. | Describes what is commonly known as the mailing address.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="StreetAddress" type="LocationText_9" minOccurs="1" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>Specifies the set of details required to fully delineate a street number, as used in postal addressing schemes.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SubsiteAddress" type="SubsiteAddressComponent_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Detailed breakout of the information that is often found on the address lines of a mailing address such as apartment, building, department, floor, mail stop, room, and suite.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="City" type="LocationText_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Text identifying a city.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CountrySubdivision" type="LocationCode_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Largest subdivision of a country.  Examples include US states and Canadian provcinces.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PostalCode" type="LocationIdentifier_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Defines international postal code.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Country" type="LocationCode_3" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Code identifying a country.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualBlock_4">
		<xs:annotation>
			<xs:documentation>Identifies the role and identity of an individual. | Identifies an individual by name only.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IndividualName" type="NameText_6" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name of the individual involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrganizationBlock_15">
		<xs:annotation>
			<xs:documentation>Specifies the role and identity of an organization. | Organization involved in the document exchange</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationName" type="NameText_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name of the organization involved in the document exchange</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationIdentifier" type="PartyIdentifier_29" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies an organization involved in the document exchange</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchOriginatorCompany.Block">
		<xs:annotation>
			<xs:documentation>Specifies the role and identity of an organization. | This is the company name and identifier of the Originator.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchOriginatorCompanyName" type="OriginatorCompanyName.Text" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This is the company name of the Originator.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OriginatorCompanyIdentifier" type="OriginatorCompany.Identifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the Originating organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchCcdReceiverOrganization.Block">
		<xs:annotation>
			<xs:documentation>Specifies the role and identity of an organization. | This is the company name and identifier of the Receiver.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchCcdReceiverCompanyName" type="ACHCCDReceiverCompanyName.Text" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>For an ACH CCD payment, the Originator provides the name of the Receiver company.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TelephoneBlock_1">
		<xs:annotation>
			<xs:documentation>Comprehensive telephone block, designed to provide definition for the telephone number, and also a variety of characteristics covering use and features. | Comprehensive telephone block, designed to provide definition for the telephone number, and also a variety of characteristics covering use and features.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TelephoneNumber" type="LocationText_4" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A word or phrase that constitutes the designation of place.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TelephoneTimeRole" type="DateTimeCode_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TelephoneLocationRole" type="LocationCode_4" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TelephoneDevice" type="ResourceCode_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TelephoneService" type="ResourceCode_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description not provided</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ElectronicLocationBlock_1">
		<xs:annotation>
			<xs:documentation>Combination of information that defines a specified individual's or organization's electronic location. | Combination of information that defines a specified individual's or organization's electronic location.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EmailAddress" type="LocationText_10" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A word or phrase that constitutes the designation of place.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OriginatorAccountDetail.Block">
		<xs:annotation>
			<xs:documentation>Identification of an organization and an account number at that organization. | Used to identify the ACH entry creator.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchOriginatorAccountType" type="AchOriginatorAccountType.Code" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes the type of account.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchOriginatorAccountNumber" type="AchOriginatorAccountNumber.Identifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This field is used to designate the Originator's account that is to be used for this transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchOriginatorRoutingAndTransitNumber" type="ResourceIdentifier_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The Routing Number is used to identify the Originator's Depository Financial Institution. (Do we want this from the Originator? Answer: Yes. Allows for banks that have accounts in different states.)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReceiverAccountDetail.Block">
		<xs:annotation>
			<xs:documentation>Identification of an organization and an account number at that organization. | This field is used to designate the Receiver's account that is to be used for this ACH transaction.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AchReceiverRoutingAndTransitNumber" type="ResourceIdentifier_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The Originator provides the Receiving Depository Financial Institution's Routing Number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchReceiverAccountNumber" type="AchReceiverAccount.Identifier" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>This field is used to designate the Receiver's account that is to be used for this  ACH transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchCcdTransactionDetail.Block">
		<xs:annotation>
			<xs:documentation>Used for processing the financial information required for a transaction event. | Detail needed to process the monetary section of an ACH CCD entry.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EffectiveEntryDate" type="EffectiveEntryDate.Text" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The effective entry date is the date specified by the Originator on which it intends a batch of entries to be settled.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchCcdTransactionCode" type="AchCcdTransactionCode.Component" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides for the conversion of the ACH CCD Transaction Code to human readable instructions.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchAmount" type="Ach.Amount" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Monetary amount required for ACH network processing. The decimal is not used in the NACHA format. Cents are always included. If the "Prenote" indicator is set to "Y" the dollar amount must be zero.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchCompanyEntryDescription" type="AchCompanyEntryDescription.Text" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The payment Originator establishes this field as the date it would like to see displayed to the Receiver for descriptive purposes only.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AchCompanyDescriptiveDate" type="AchCompanyDescriptiveDate.Text" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The payment Originator establishes this field as the date it would like to see displayed to the Receiver for descriptive purposes only.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Components ========== -->
	<xs:complexType name="SubsiteAddressComponent_1">
		<xs:annotation>
			<xs:documentation>Detailed breakout of the information that is often found on the Address Lines of a mailing address such as apartment, building, department, floor, mail stop, room, and suite. | Detailed breakout of the information that is often found on the address lines of a mailing address such as apartment, building, department, floor, mail stop, room, or suite.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Apartment" type="LocationText_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A single residence in a multi unit dwelling.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Building" type="LocationText_7" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A single structure.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Department" type="LocationText_7" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A distinct usually specified division of a large organization.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Floor" type="LocationText_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A story or level of a building.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Room" type="LocationText_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>An area seperated by walls or partitions from other similar parts of the building in which it is located.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Suite" type="LocationText_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A series of connected rooms used as a living unit.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AchCcdTransactionCode.Component">
		<xs:annotation>
