<xs:schema xmlns="urn:x12:schemas:V00201202:BargeInvoiceDocument" xmlns:X12.7="urn:x12:schemas:V00201002:X12.7" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:x12:schemas:V00201202:BargeInvoiceDocument" elementFormDefault="qualified" attributeFormDefault="unqualified" version="00201202" X12.7:PublicationDate="2012-02-12">
	<!-- ==========  Document ========== -->
	<xs:element name="BargeInvoiceDocument">
		<xs:annotation>
			<xs:documentation>This CICA message template contains the format and establishes the data contents of the Freight Invoice. For use within the context of an electronic data interchange environment. This standard can be used to electronically send freight bill data. | The Barge Invoice Document provides a means for a Marine Trading Partner to transmit an invoice to another Marine Trading Partner for services performed. The Barge Invoice Document allows the sender to specify Interested Parties as well as Invoice Summary and Invoice Line Items.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="DocumentHeader" type="StandardDocumentHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information for this document.  Uniquely identifies this document being transmitted.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ApplicationHeader" type="MarineApplicationHeaderModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Provides control information and uniquely identifies a document from an application's perspective.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="InvoiceInformation" type="MarineInvoiceSummaryModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Contains the Invoice identification, terms, and summary details.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RemitToParty" type="InvoiceInterestedPartyWithTaxInformationModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The party that should receive payment for the invoice.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BilledToParty" type="InvoiceInterestedPartyModule" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The party that is being billed (i.e., the party that is receiving the invoice).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BilledFromParty" type="InvoiceInterestedPartyModule" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The party who generated the invoice.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Detail" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="LineItem" type="MarineInvoiceLineItemModule" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Invoice line item.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="PublicationVersion" type="PublicationVersionType" use="required"/>
			<xs:attribute name="PublicationDate" type="xs:date" use="required"/>
		</xs:complexType>
	</xs:element>
	<!-- ==========  Modules ========== -->
	<xs:complexType name="MarineInvoiceLineItemModule">
		<xs:annotation>
			<xs:documentation>An invoice line item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Details" type="LineItemBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Represents the invoice line item details, including the line item number and line item amount due.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="VesselEventStatusSelectorAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Invoice line item type.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateInformation" type="MarineBillingInformationAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional line item rate details.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrderDocumentReference" type="DocumentDetailBlock_33" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional reference to the order document and order line item that this invoice line item fulfills.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TransactionReference" type="DocumentDetailBlock_34" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Optional transaction reference to the document to which this invoice line item applies.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EquipmentList" type="MarineResourceListAssembly_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The equipment list associated with the invoice line item, including barges, boats, and any other type that is not of type barge or boat.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineInvoiceSummaryModule">
		<xs:annotation>
			<xs:documentation>Contains identification, terms, and summary details for an Invoice.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="InvoiceIdentification" type="DocumentDetailBlock_35" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Contains identification, terms, and summary details for an Invoice.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InvoiceInterestedPartyModule">
		<xs:annotation>
			<xs:documentation>Identifies a party that has an interest in the invoice.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_17" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Name of the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationAddress" type="AddressBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Address of the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualContact" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Contact information for an individual in the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InvoiceInterestedPartyWithTaxInformationModule">
		<xs:annotation>
			<xs:documentation>Identifies a party that has an interest in the invoice, including that partyâ€™s tax information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_18" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization name and Tax ID.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OrganizationAddress" type="AddressBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Address of the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualContact" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Contact information for an individual in the organization.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StandardDocumentHeaderModule">
		<xs:annotation>
			<xs:documentation>Standard document header module</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Sender" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who sends the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Receiver" type="OrganizationAndIndividualInformationAssembly_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the party who receives the document</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentDetail" type="DocumentDetailBlock_31" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identification of document</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineApplicationHeaderModule">
		<xs:annotation>
			<xs:documentation>Provides control information and uniquely identifies a document from an application's perspective.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TransactionDetail" type="DocumentDetailBlock_36" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides control information and uniquely identifies the transaction that is represented by this document from an application's perspective.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponsibleLocation" type="LocationIdentifiersBlock_13" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The location (e.g. fleet, terminal, lock, etc.) that is responsible for this document.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ==========  Assemblies ========== -->
	<xs:complexType name="OrganizationContactAssembly_12">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an organization. | Information used to contact the organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationIdentification" type="OrganizationBlock_15" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressBlock_1" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Describes what is commonly known as the mailing address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IndividualContactAssembly_2">
		<xs:annotation>
			<xs:documentation>Information that identifies a specific name and electronic or physical location of an individual. | Information used to contact the individual.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="ContactIdentification" type="IndividualBlock_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IndividualLocation" type="EntityLocationAssembly_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Location of Individual involved in the document exchange.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrganizationAndIndividualInformationAssembly_1">
		<xs:annotation>
			<xs:documentation>Organization and Individual Information Assembly | Identifies the organization and provides an individual contact.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganizationContact" type="OrganizationContactAssembly_12" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Organization involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IndividualContact" type="IndividualContactAssembly_2" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Individual involved in the document exchange.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineBillingInformationAssembly_1">
		<xs:annotation>
			<xs:documentation>This assembly groups billing information. | Cost information with rate details.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaseRateInformation" type="RateDetailBlock_1" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Details on how to calculate the base cost of an item or service.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Incidental" type="ResourceCostAssembly_1" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Unbounded collection of incidentals (additional fees).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResourceCostAssembly_1">
		<xs:annotation>
			<xs:documentation>Provides the cost and identification information for a resource. | Provides identification and cost details about an incidental.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Details" type="ResourceIdentificationBlock_40" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides details about the incidental.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RateInformation" type="RateDetailBlock_2" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides details on how the incidental amount was calculated.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VesselEventStatusSelectorAssembly_1">
		<xs:annotation>
			<xs:documentation>Allows for the selection of a vessel status or vessel event. | Invoice line item type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Assist" type="VesselEventStatusAssembly_10" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A secondary boat (e.g. a fleet boat) has helped a primary boat (e.g. a line boat) with a task and a barge was involved.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Clean" type="VesselEventStatusAssembly_4" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has been cleaned by a vendor.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CoverHandling" type="VesselEventStatusAssembly_5" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>The covers of a barge have been physically manipulated in some manner.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Delay" type="VesselEventStatusAssembly_38" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Barge was delayed.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Fleeting" type="VesselEventStatusAssembly_9" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Barge has been in the fleet.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Load" type="VesselEventStatusAssembly_11" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had commodity placed within the cargo hold compartment.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="MidstreamOperation" type="VesselEventStatusAssembly_12" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Midstream In (Drop) - A barge has been retrieved from the tow of a mainline boat. Midstream Out (Pickup) - A barge has been positioned to be included in the tow for a mainline boat.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Other" type="VesselEventStatusAssembly_2" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A miscellaneous event or status update.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Pump" type="VesselEventStatusAssembly_6" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had water removed from the void tank areas or cargo hold area.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Repair" type="VesselEventStatusAssembly_7" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had some type of work performed to correct a physical problem.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Shift" type="VesselEventStatusAssembly_1" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has been moved from one location to another by a vendor.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TowAssist" type="VesselEventStatusAssembly_3" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A secondary boat has assisted a mainline boat with the making or breaking of a tow.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Unload" type="VesselEventStatusAssembly_8" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A barge has had commodity removed from the cargo hold compartment.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MarineResourceListAssembly_1">
		<xs:annotation>
			<xs:documentation>A list of marine resources (e.g. boats, barges, vessels, etc.). | An unbounded list of marine resources (e.g. boats, barges, etc.).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="Barge" type="MarineVesselBlock_1" minOccurs="1" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>List of barges.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Boat" type="MarineVesselBlock_2" minOccurs="1" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>List of boats.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Other" type="ResourceIdentificationBlock_39" minOccurs="1" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>List of other items that are not barges or boats.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VesselEventStatusAssembly_1">
		<xs:annotation>
			<xs:documentation>Event status information for a vessel. | Barge shift event (a barge was moved from one location to another) as used in an invoice.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EventDetails" type="EventDetailBlock_4" minOccurs="1" maxOccurs="1">
				<xs:annotation>
